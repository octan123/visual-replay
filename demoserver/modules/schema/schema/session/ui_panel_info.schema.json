{
  "id": "https://xviz.org/schema/session/ui_panel_info.json",
  "$schema": "http://json-schema.org/draft-04/schema#",
  "description": "XVIZ Session UI Panel Info",
  "type": "object",
  "properties": {
    "name": {
      "type": "string"
    },
    "needed_streams": {
      "type": "array",
      "items": {
        "type": "string"
      },
      "additionalItems": false
    },
    "config": {
      "oneOf": [
        {"type": "object"},
        {"type": "string"}
      ]
    }
  },
  "required": [
    "name",
    "config"
  ],
  "additionalProperties": false
}
